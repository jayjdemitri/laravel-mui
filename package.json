{
    "name": "laravel-mui",
    "version": "0.4.0",
    "description": "A Laravel Skeleton Application",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:watch": "concurrently --kill-others \"npm run dev\" \"php artisan serve\"",
        "build": "tsc && vite build",
        "pretest": "npm run build",
        "lint": "eslint",
        "test": "php artisan test",
        "preversion": "npm run test",
        "version": "git add .",
        "postversion": "git push && git push --tags"
    },
    "devDependencies": {
        "@eslint/js": "^9.8.0",
        "@headlessui/react": "^2.0.0",
        "@inertiajs/react": "^1.0.0",
        "@tailwindcss/forms": "^0.5.3",
        "@types/node": "^18.13.0",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.10",
        "@vitejs/plugin-react": "^4.2.0",
        "autoprefixer": "^10.4.12",
        "axios": "^1.6.4",
        "concurrently": "^8.2.2",
        "eslint": "^9.8.0",
        "eslint-plugin-react": "^7.35.0",
        "globals": "^15.8.0",
        "laravel-vite-plugin": "^1.0",
        "postcss": "^8.4.31",
        "prettier": "^3.3.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwindcss": "^3.2.1",
        "typescript": "^5.0.2",
        "typescript-eslint": "^7.17.0",
        "vite": "^5.0"
    },
    "dependencies": {
        "@emotion/react": "^11.13.0",
        "@emotion/styled": "^11.13.0",
        "@fontsource/roboto": "^5.0.13",
        "@mui/icons-material": "^5.16.4",
        "@mui/material": "^5.16.4"
    }
}